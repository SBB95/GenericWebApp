<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MasterServlet</servlet-name>
  	<display-name>MasterServlet</display-name>
  	<description></description>
  	<servlet-class>source.layers.servlets.MasterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RequestHelper</servlet-name>
  	<display-name>RequestHelper</display-name>
  	<description></description>
  	<servlet-class>source.layers.servlets.RequestHelper</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>source.layers.controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MasterServlet</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RequestHelper</servlet-name>
  	<url-pattern>/RequestHelper</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/UserController</url-pattern>
  </servlet-mapping>
</web-app>
